{
  "id": null,
  "uid": "edge-serve",
  "title": "Edge Serve Dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "tags": ["edge", "serve"],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "p99 Prediction Latency (ms)",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(edge_serve_prediction_latency_seconds_bucket[5m])) * 1000",
          "legendFormat": "p99 latency",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (req/s)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(edge_serve_request_errors_total[5m])",
          "legendFormat": "errors/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "gauge",
      "title": "Expected Calibration Error (ECE)",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "edge_serve_ece",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 10,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 2},
              {"color": "green", "value": 1}
            ]
          }
        },
        "overrides": []
      }
    }
  ]
}