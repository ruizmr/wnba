{
  "dashboard": {
    "id": null,
    "uid": "wehoop-train",
    "title": "WEHOOP Training & Ingestion",
    "tags": ["wehoop", "training"],
    "timezone": "browser",
    "schemaVersion": 37,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "type": "graph",
        "title": "Validation Loss per Trial",
        "id": 1,
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 0},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "validation_loss{job=\"minihgt\"}",
            "legendFormat": "{{trial}}",
            "refId": "A"
          }
        ],
        "lines": true,
        "linewidth": 1,
        "nullPointMode": "null",
        "xaxis": {"show": true},
        "yaxes": [
          {"format": "short", "label": "loss", "show": true},
          {"show": false}
        ]
      },
      {
        "type": "stat",
        "title": "Rows/s Ingest Throughput",
        "id": 2,
        "gridPos": {"h": 5, "w": 8, "x": 0, "y": 9},
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "ingest_rows_per_sec{job=\"minihgt\"}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"], "fields": ""}
        }
      }
    ]
  },
  "overwrite": true
}